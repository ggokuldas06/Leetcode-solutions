class Solution {
public:
    int minimumTotal(vector<vector<int>>& triangle) {
        int n=triangle.size();
        if(n==0) return 0;
        vector<vector<int>>dp=triangle;
        for(int r=n-2;r>=0;r--){
            for(int c=0;c<triangle[r].size();c++){
                dp[r][c]=min(dp[r+1][c+1],dp[r+1][c])+triangle[r][c];
            }
        }
        return dp[0][0];
    }
};
