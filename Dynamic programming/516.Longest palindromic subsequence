class Solution {
public:
    int lcs(string& a, string& b){
        int n=a.size();
        vector<vector<int>>dp(n+1,vector<int>(n+1,0));
        for(int i=n-1;i>=0;i--){
            for(int j=n-1;j>=0;j--){
                if(a[i]==b[j]){
                    dp[i][j]=1+dp[i+1][j+1];
                }else{
                    dp[i][j]=max(dp[i+1][j],dp[i][j+1]);
                }
            }
        }
        return dp[0][0];
    }
    int longestPalindromeSubseq(string s) {
        string s2=reverse(s.begin(),s.end());
        return lcs(s,s2);
    }
};
