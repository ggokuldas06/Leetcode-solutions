class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        int n=nums.size();
        int count=0;
        unordered_map<int,int>hash;
        hash[0]=1;
        int prefix=0;
        for(int i=0;i<n;i++){
            prefix+=nums[i];
            count+=hash[prefix-k];
            hash[prefix]++;
        }
        return count;
    }
};
