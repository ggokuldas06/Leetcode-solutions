class Solution {
public:
    bool checkSubarraySum(vector<int>& nums, int k) {
        int curr=0;
        unordered_map<int,int>hash;
        hash[0]=-1;
        int ans=0;
        for(int i=0;i<nums.size();i++){
            curr+=nums[i];
            int rem=curr%k;
            if(hash.count(rem)){
                if(i-hash[rem]>=2)return true;
            }
            hash[curr]++;
        }
        return false;
    }
};
