class Solution {
public:
    int largestAltitude(vector<int>& gain) {
        int sum=0,maxi=0;
        for(int i=0;i<gain.size();i++)
        {
            sum+=gain[i];
            maxi=max(sum,maxi);
        }
        return maxi;
    }
};

*********
Dynamic programming
O(N)
*********
calculate the sum at each point and then calculate the max till that
